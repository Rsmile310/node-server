{
  "name": "@nestjs/serve-static",
  "version": "1.0.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@serve-static)",
  "author": "Kamil Mysliwiec",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:lib",
    "build:lib": "tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "express": "^4.16.4",
    "zone.js": "^0.8.29 || ^0.9.0"
  },
  "devDependencies": {
    "@nestjs/common": "6.5.2",
    "@nestjs/core": "6.5.2",
    "@types/node": "^12.6.8",
    "express": "4.17.1",
    "husky": "3.0.0",
    "lint-staged": "9.2.0",
    "prettier": "1.18.2",
    "ts-morph": "3.1.1",
    "typescript": "3.4.4",
    "zone.js": "0.9.1"
  },
  "dependencies": {
    "concurrently": "4.1.1",
    "cross-spawn": "6.0.5",
    "livereload": "0.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
